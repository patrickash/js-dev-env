{
  "name": "js-dev-env",
  "version": "1.0.0",
  "description": "JavaScript development environment",
  "scripts": {
    "prestart": "babel-node tools/startMessage.js",
    "start": "npm-run-all --parallel open:src lint:watch test:watch start-mockapi",
    "open:src": "babel-node tools/srcServer.js",
    "localtunnel": "lt --port 3000",
    "share": "npm-run-all --parallel start localtunnel",
    "lint": "esw webpack.config.* src tools --color",
    "lint:watch": "npm run lint -- --watch",
    "test": "mocha tools/testSetup.js",
    "test:watch": "npm run test -- --watch",
    "generate-mock-data": "babel-node tools/generateMockData",
    "prestart-mockapi": "npm run generate-mock-data",
    "start-mockapi": "json-server --watch src/api/db.json --port 3001",
    "clean-dist": "rimraf ./dist && mkdir dist",
    "prebuild": "npm-run-all clean-dist test lint",
    "build": "babel-node tools/build.js",
    "postbuild": "babel-node tools/distServer.js",
    "deploy": "surge ./dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/patrickash/js-dev-env.git"
  },
  "author": "Patrick Ashamalla",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/patrickash/js-dev-env/issues"
  },
  "homepage": "https://github.com/patrickash/js-dev-env#readme",
  "dependencies": {
    "whatwg-fetch": "^3.6.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.3",
    "@babel/core": "^7.17.5",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/node": "^7.16.8",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.16.11",
    "@babel/register": "^7.17.0",
    "auto-bind": "^5.0.1",
    "babel-loader": "^8.2.3",
    "chai": "^4.3.6",
    "chalk": "^4.1.2",
    "cheerio": "^1.0.0-rc.10",
    "clean-webpack-plugin": "^4.0.0",
    "compression": "^1.7.4",
    "concurrently": "^7.0.0",
    "copy-webpack-plugin": "^10.2.4",
    "cross-env": "^7.0.3",
    "css-loader": "^6.6.0",
    "dotenv": "^16.0.0",
    "eslint": "^8.9.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-watch": "^8.0.0",
    "express": "^4.17.3",
    "faker": "^6.6.6",
    "file-loader": "^6.2.0",
    "glslify-loader": "^2.0.0",
    "handlebars-loader": "^1.7.1",
    "html-webpack-plugin": "^5.5.0",
    "jsdom": "^19.0.0",
    "json-schema-faker": "^0.5.0-rcv.41",
    "json-server": "^0.17.0",
    "localtunnel": "^2.0.2",
    "mini-css-extract-plugin": "^2.5.3",
    "mocha": "^9.2.1",
    "normalize-wheel": "^1.0.1",
    "npm-run-all": "^4.1.5",
    "numeral": "^2.0.6",
    "ogl": "^0.0.86",
    "open": "^8.4.0",
    "postcss-loader": "^6.2.1",
    "raw-loader": "^4.0.2",
    "rimraf": "^3.0.2",
    "sass": "^1.49.8",
    "sass-loader": "^12.6.0",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.3.1",
    "webpack": "^5.76.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-middleware": "^5.3.1",
    "webpack-dev-server": "^4.7.4",
    "webpack-merge": "^5.8.0"
  }
}
